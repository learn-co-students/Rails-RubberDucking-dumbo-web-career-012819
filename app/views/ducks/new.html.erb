<h2>Create A Duck!</h2>

<%= @duck.errors.full_messages do |message| %>
<ul>
    <li><%= message %></li>
</ul>
<% end %>

<%= form_for(@duck) do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :description %>
<%= f.text_field :description %>

<%= f.collection_select(:student_id, @students, :id, :name) %>
<%= f.submit %>

<% end %>
